{"data": {
  "data_dir": "data",
  "dataset": "all_stocks_5yr.csv",
  "output_dir": "model"
  },
"preprocessing": {
  "vars_output": ["close"],
  "no_multisteps": 1,
  "lag": 1,
  "stationary": false,
  "ahead": 1,
  "refit": false,
  "multivariate": false,
  "test_size": 0.1
  },
"arch": {
  "neurons": 50,
  "rnn": "MLP",
  "drop": 0,
  "nlayers": 1,
  "activation": "relu",
  "activation_r": "hard_sigmoid",
  "impl": 2
  },
"training": {
  "epochs": 16,
  "optimizer": "adam",
  "lrate": 0.001
  }
}