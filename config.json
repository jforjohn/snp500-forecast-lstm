  "rnn": "LSTM",
  "drop": 0.0,
  "nlayers": 1,
  "activation": "tanh",
  "activation_r": "hard_sigmoid"
  },
"training": {
  "batch": 500,
  "epochs": 25,
  "optimizer": "adam",
  "lrate": 0.001
  }
}